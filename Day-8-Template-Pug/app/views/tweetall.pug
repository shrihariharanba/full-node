doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    title User Detail
  body
    section.container
      for tweet in result
        P #{tweet.message}
        P #{tweet.comments}
        P #{tweet.userId}
        p
         a(href='http://localhost:3002/tweet/v1/tweet/'+tweet._id) details